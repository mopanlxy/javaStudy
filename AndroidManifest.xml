<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.linc.readdata"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="9" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_javasystem"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.linc.readdata.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.linc.readdata.TestActivity"
            android:label="@string/title_activity_test" >
        </activity>
        <activity
            android:name="com.linc.readdata.PractiseActivity"
            android:label="@string/title_activity_practise" >
        </activity>
        <activity
            android:name="com.linc.readdata.ErrorActivity"
            android:label="@string/title_activity_error" >
        </activity>
        <activity
            android:name="com.linc.readdata.AchievementActivity"
            android:label="@string/title_activity_achievement" >
        </activity>
        <activity
            android:name="com.linc.readdata.PractiselistActivity"
            android:label="@string/title_activity_practiselist" >
        </activity>
        <activity
            android:name="com.linc.readdata.ImportActivity"
            android:label="@string/title_activity_import" >
        </activity>
        <activity
            android:name="com.linc.readdata.ImportTestActivity"
            android:label="@string/title_activity_import_test" >
        </activity>
    </application>

</manifest>
